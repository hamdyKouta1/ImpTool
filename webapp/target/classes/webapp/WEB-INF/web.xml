<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee          http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>Easy Search App</display-name>
    <!-- Servlet for Query 1 -->
    <servlet>
        <servlet-name>RunQuery1Servlet</servlet-name>
        <servlet-class>com.example.webapp.RunQuery1Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClearCacheGWServlet</servlet-name>
        <servlet-class>com.example.webapp.ClearCacheGWServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClearCacheSOFServlet</servlet-name>
        <servlet-class>com.example.webapp.ClearCacheSOFServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomRquestServlet</servlet-name>
        <servlet-class>com.example.webapp.CustomRquestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RunQuery1Servlet</servlet-name>
        <url-pattern>/runQuery1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ConfigServlet</servlet-name>
        <servlet-class>com.example.webapp.ConfigServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConfigServlet</servlet-name>
        <url-pattern>/ConfigServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NotesServlet</servlet-name>
        <servlet-class>com.example.webapp.NotesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NotesServlet</servlet-name>
        <url-pattern>/NotesServlet</url-pattern>
    </servlet-mapping>
    <!-- Servlet for Query 2 -->
    <servlet>
        <servlet-name>RunQuery2Servlet</servlet-name>
        <servlet-class>com.example.webapp.RunQuery2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RunQuery2Servlet</servlet-name>
        <url-pattern>/runQuery2</url-pattern>
        </servlet-mapping>
    <servlet>
        <servlet-name>ClearBillerCacheServlet</servlet-name>
        <servlet-class>com.example.webapp.ClearBillerCacheServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClearBillerCacheServlet</servlet-name>
        <url-pattern>/ClearBillerCacheServlet</url-pattern>
    </servlet-mapping>
    <!-- Servlet for Query 3 -->
    <servlet>
        <servlet-name>GetCustomerBKIDServlet</servlet-name>
        <servlet-class>com.example.webapp.GetCustomerBKIDServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetCustomerBKIDServlet</servlet-name>
        <url-pattern>/getCustomerBKID</url-pattern>
    </servlet-mapping>
    <!-- Servlet for Query 4 -->
    <servlet>
        <servlet-name>GetCustomerAccountIDServlet</servlet-name>
        <servlet-class>com.example.webapp.GetCustomerAccountIDServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetCustomerAccountIDServlet</servlet-name>
        <url-pattern>/getCustomerAccountID</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetBillInfoServlet</servlet-name>
        <servlet-class>com.example.webapp.GetBillInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetBillInfoServlet</servlet-name>
        <url-pattern>/GetBillInfoServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getUserNIDServlet</servlet-name>
        <servlet-class>com.example.webapp.getUserNIDServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getUserNIDServlet</servlet-name>
        <url-pattern>/getUserNIDServlet</url-pattern>
    </servlet-mapping>
    <!-- Welcome File (Home Page) -->
    <servlet-mapping>
        <servlet-name>ClearCacheGWServlet</servlet-name>
        <url-pattern>/ClearCacheGWServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClearCacheSOFServlet</servlet-name>
        <url-pattern>/ClearCacheSOFServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomRquestServlet</servlet-name>
        <url-pattern>/CustomRquestServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>${project.basedir}/src/main/webapp/index.html</welcome-file>
    </welcome-file-list>
</web-app>
